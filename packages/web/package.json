{
  "name": "@webview-bridge/web",
  "type": "module",
  "version": "1.7.9",
  "description": "Fully Type-Safe Integration for React Native WebView and Web",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/gronxb/webview-bridge",
  "license": "MIT",
  "files": [
    "dist",
    "package.json"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup --config ./tsup.config.ts",
    "test:type": "tsc --noEmit",
    "lint": "biome check"
  },
  "dependencies": {
    "@webview-bridge/utils": "workspace:*",
    "@webview-bridge/types": "workspace:*"
  }
}
